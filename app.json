{
  "expo": {
    "name": "T-Window",
    "slug": "T-Window",
    "version": "1.0.0",
    "orientation": "portrait",
    
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
     
        "backgroundColor": "#ffffff"
      },
      "package": "com.forkii.TWindow"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
     
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "49185835-8e30-4d07-b094-2c1b3239c433"
      }
    }
  }
}
